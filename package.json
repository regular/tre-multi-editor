  "tre-init": {
    "plugins": {
      "tre-client": true,
      "ssb-revisions": true
    },
    "prototypes": {
      "tre-images": true,
      "tre-fonts": true,
      "tre-stylesheets": true
    },
    "importers": {
      "tre-images": true,
      "tre-fonts": true,
      "tre-stylesheets": true
    },
    "files": {
      "svg": {
        "path": "assets/Svg_example4.svg",
        "content": {
          "branch": "%images",
          "root": "%root"
        }
      },
      "beaver": {
        "path": "assets/beaver.jpg",
        "content": {
          "branch": "%images",
          "root": "%root"
        }
      },
      "base-css": {
        "path": "assets/base.css",
        "content": {
          "branch": "%stylesheets",
          "root": "%root"
        }
      },
      "font": {
        "path": "assets/NotoSansDisplay-ThinItalic.ttf",
        "content": {
          "branch": "%fonts",
          "root": "%root"
        }
      }
    }
  },
  "devDependencies": {
    "brace": "^0.11.1",
    "ssb-revisions": "^1.2.1",
    "tre-client": "^2.1.1",
